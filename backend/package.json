{
  "name": "api-template",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "fix:eslint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:prettier": "prettier --check src",
    "fix:prettier": "prettier --write src",
    "lint:all": "npm run lint:tsc && npm run lint:eslint && npm run lint:prettier"
  },
  "repository": "git@github.com:nakamurakzz/template-api.git",
  "author": "nakamurakzz <kazutaka.nakamura@ncdc.co.jp>",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/jest": "^29.2.5",
    "express": "^4.18.2",
    "jest": "^29.3.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11"
  }
}